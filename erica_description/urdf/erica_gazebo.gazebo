    <mu2>0.2</mu2>
    <material>Gazebo/White</material>
  </gazebo>

 <gazebo reference="r_arm_wrist_roll">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <material>Gazebo/White</material>
  </gazebo>
 

</robot>
