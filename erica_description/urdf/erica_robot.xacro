  <!-- main -->

 <link name="body_main">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
         <geometry>
          <mesh filename="package://erica_description/meshes/asm_don_big_main.stl" scale="0.001 0.001 0.001"/>
        </geometry>
    </collision>

    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://erica_description/meshes/asm_don_big_main.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <material name="white"/>
    </visual>

    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>      
      <mass value="300"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />      
    </inertial>
  </link>

  <!-- left wheel front-->

  <joint name="left_wheel_front_joint" type="revolute">
    <origin xyz="0.09 0.157 0.037" rpy="0 0 0"/>
    <parent link="body_main"/>
    <child link="left_wheel_front"/>
    <axis xyz="0 1 0"/>
    <limit velocity="6.5" effort="1000" lower="${-pi}" upper="${pi}" />
    <dynamics damping = "0.7"/>
  </joint>

<transmission name="left_wheel_front_joint_tran">
     <type>transmission_interface/SimpleTransmission</type>
     <joint name="left_wheel_front_joint">
       <hardwareInterface>EffortJointInterface</hardwareInterface>
     </joint>
     <actuator name="left_wheel_front_joint_motor">
       <hardwareInterface>EffortJointInterface</hardwareInterface>
       <mechanicalReduction>1</mechanicalReduction>
     </actuator>
  </transmission>

