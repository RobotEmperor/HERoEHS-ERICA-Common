<?xml version="1.0" ?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find erica_gazebo)/config/position_controller.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
	output="screen" ns="/erica_robot" args="joint_state_controller
				       		          l_arm_shoulder_pitch_position
				       		    	  r_arm_shoulder_pitch_position
				       		 	  l_arm_shoulder_roll_position
							  r_arm_shoulder_roll_position
							  l_arm_shoulder_yaw_position
							  r_arm_shoulder_yaw_position
							  l_arm_elbow_pitch_position
				       		    	  r_arm_elbow_pitch_position
				       		 	  l_arm_wrist_pitch_position
							  r_arm_wrist_pitch_position
							  l_arm_wrist_roll_position
 							  r_arm_wrist_roll_position
							  "/>

</launch>
